o Utils-YourProjectName.cmi : cp ../bs/reason/Utils-YourProjectName.cmi
o Utils-YourProjectName.cmj : cp ../bs/reason/Utils-YourProjectName.cmj
o Utils-YourProjectName.cmt : cp ../bs/reason/Utils-YourProjectName.cmt
o Utils.re : cp ../../reason/Utils.re
o Reducer-YourProjectName.cmi : cp ../bs/reason/Reducer-YourProjectName.cmi
o Reducer-YourProjectName.cmj : cp ../bs/reason/Reducer-YourProjectName.cmj
o Reducer-YourProjectName.cmt : cp ../bs/reason/Reducer-YourProjectName.cmt
o Reducer.re : cp ../../reason/Reducer.re
o Button-YourProjectName.cmi : cp ../bs/reason/components/Button/Button-YourProjectName.cmi
o Button-YourProjectName.cmj : cp ../bs/reason/components/Button/Button-YourProjectName.cmj
o Button-YourProjectName.cmt : cp ../bs/reason/components/Button/Button-YourProjectName.cmt
